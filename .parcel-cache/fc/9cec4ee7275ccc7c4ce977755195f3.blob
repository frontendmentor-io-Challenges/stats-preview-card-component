@import url("https://fonts.googleapis.com/css2?family=Inter:wght@400;700&family=Lexend+Deca&display=swap");
html {
  box-sizing: border-box;
  font-size: 16px;
}
*, *:before, *:after {
  box-sizing: inherit;
}
body, h1, h2, h3, h4, h5, h6, p, ol, ul {
  margin: 0;
  padding: 0;
  font-weight: normal;
}
ol, ul {
  list-style: none;
}
img {
  max-width: 100%;
  height: auto;
}
body {
  background: #090b1a;
  font-family: "Inter", sans-serif;
  font-size: 15px;
  color: white;
}
.project-container {
  margin: 88px 24px;
}
.card {
  margin: 0 auto;
  width: 327px;
  background-color: #1b1938;
  border-radius: 8px;
  overflow: hidden;
}
.card__image {
  height: 240px;
  background-image: linear-gradient(to right, #aa5cdb, #aa5cdb), url("5061629309cd8252db999f011025fa19");
  background-blend-mode: multiply;
  background-position: center center;
  background-size: cover;
}
.card__text__wrapper {
  padding: 40px 31px 32px;
}